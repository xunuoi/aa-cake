{  
  "name": "AA-Cake",  
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Add a little sweetness to your work",
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "AA Cake",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["eventPage.js"]
  },  
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lib/jquery-2.1.1.min.js", "lib/copy-to-clipboard.js", "lib/rison.js", "main.js"]
    }
  ]
} 